{
  "version": 2,
  "name": "anjit pariyar",
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    },
    "scripts": {
      "build": "rimraf dist && tsc"
    }
  }
}
